{
  "name": "basic",
  "type": "module",
  "version": "1.0.3",
  "scripts": {
    "dev": "vite",
    "build:workers": "vite build --mode client && vite build --mode workers",
    "build:bun": "vite build --mode client && vite build --mode bun",
    "preview": "wrangler dev",
    "deploy:workers": "$npm_execpath run build:workers && wrangler deploy",
    "typecheck": "tsc --noEmit",
    "lint": "biome check",
    "lint:fix": "biome check --write --unsafe",
    "format": "biome format --write",
    "migrateup": "dbmate up",
    "migratedown": "dbmate down"
  },
  "private": true,
  "dependencies": {
    "@remix-run/react": "^2.17.2",
    "bcrypt-ts": "^6.0.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "encoding-japanese": "^2.2.0",
    "hono": "^4.7.4",
    "hono-pino": "^0.8.0",
    "honox": "^0.1.40",
    "iconv-lite": "^0.6.3",
    "neverthrow": "^8.2.0",
    "notyf": "^3.10.0",
    "pino": "^9.6.0",
    "postgres": "^3.4.5",
    "uuidv7": "^1.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@cloudflare/workers-types": "^4.20250313.0",
    "@hono/node-server": "^1.13.8",
    "@hono/vite-build": "^1.3.0",
    "@hono/vite-dev-server": "^0.18.3",
    "@tailwindcss/vite": "^4.0.14",
    "@ts-safeql/eslint-plugin": "^3.6.10",
    "@types/encoding-japanese": "^2.2.1",
    "@types/node": "^22.13.10",
    "dbmate": "^2.26.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.13.0",
    "eslint-import-resolver-typescript": "^3.9.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-x": "^4.8.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "libpg-query": "^16.3.0",
    "tailwindcss": "^4.0.14",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.2.2",
    "vitest": "^3.1.1",
    "wrangler": "^4.0.0"
  },
  "packageManager": "pnpm@10.7.1"
}
